{
  "manifest_version": 3,
  "name": "Sample Web Extension",
  "version": "0.0.1",
  "background": {
    "service_worker": "./src/scripts/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "*://youtube.com/*",
        "*://google.com/*",
        "*://edwinkofler.com/*"
      ],
      "js": ["./src/content_scripts/youtube.js"]
    }
  ]
}
